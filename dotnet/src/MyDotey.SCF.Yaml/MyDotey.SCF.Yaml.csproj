<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <AssemblyTitle>MyDotey.SCF.Yaml</AssemblyTitle>
    <AssemblyName>MyDotey.SCF.Yaml</AssemblyName>
    <PackageId>MyDotey.SCF.Yaml</PackageId>
    <Version>1.1.0</Version>
    <Authors>koqizhao</Authors>
    <Description>SCF Yaml Configuration Soruce</Description>
    <Copyright>koqizhao</Copyright>

    <PackageTags>scf;configuration;facade;yaml</PackageTags>
    <PackageReleaseNotes>SCF Yaml Configuration Soruce</PackageReleaseNotes>
    <PackageIconUrl>http://numl.net/images/ico.png</PackageIconUrl>
    <PackageProjectUrl>https://github.com/mydotey/scf-yaml</PackageProjectUrl>
    <PackageLicenseUrl>https://opensource.org/licenses/Apache-2.0</PackageLicenseUrl>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <RepositoryType>git</RepositoryType>
